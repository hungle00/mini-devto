<h3 class="mb-4">
  <span>Statistics</span>
</h3>
<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">7 Days Views</div>
      <div class="card-body" data-controller="chart">
        <canvas data-chart-target="output" data-labels="<%= @count_by_date.keys %>" 
                data-views="<%= @count_by_date.values %>"></canvas>
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">7 Days Unique Views</div>
      <div class="card-body" data-controller="chart">
        <canvas data-chart-target="output" data-labels="<%= @uniq_count_by_date.keys %>" 
                data-views="<%= @uniq_count_by_date.values %>"></canvas>
      </div>
    </div>
    </div>
  </div>
</div>
